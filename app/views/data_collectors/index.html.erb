<h1>Listing data_collectors</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Script</th>
    <th>Unit</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @data_collectors.each do |data_collector| %>
  <tr>
    <td><%= data_collector.name %></td>
    <td><%= data_collector.description %></td>
    <td><%= data_collector.script %></td>
    <td><%= data_collector.unit %></td>
    <td><%= link_to 'Show', data_collector %></td>
    <td><%= link_to 'Edit', edit_data_collector_path(data_collector) %></td>
    <td><%= link_to 'Destroy', data_collector, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Data collector', new_data_collector_path %>
